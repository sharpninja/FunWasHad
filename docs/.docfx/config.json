{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "../../FWH.Location.Api/FWH.Location.Api.csproj",
            "../../FWH.MarketingApi/FWH.MarketingApi.csproj",
            "../../FWH.Common.Location/FWH.Common.Location.csproj",
            "../../FWH.Common.Chat/FWH.Common.Chat.csproj",
            "../../FWH.Common.Workflow/FWH.Common.Workflow.csproj",
            "../../FWH.Common.Imaging/FWH.Common.Imaging.csproj",
            "../../FWH.Mobile.Data/FWH.Mobile.Data.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/*Tests.csproj"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "index.md",
          "Technical-Requirements.md",
          "Functional-Requirements.md",
          "README.md"
        ]
      },
      {
        "files": [
          "api/**.md",
          "summaries/**.md",
          "reviews/**.md",
          "architecture/**.md",
          "configuration/**.md",
          "platform/**.md",
          "testing/**.md",
          "references/**.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/bin/**",
          "**/_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "xref": []
  }
}
